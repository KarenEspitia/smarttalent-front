{
  "semi": true,
  "singleQuote": true,
  "printWidth": 90,
  "tabWidth": 2,
  "useTabs": false,
  "trailingComma": "es5",
  "bracketSpacing": true,
  "endOfLine": "auto",
  "quoteProps": "as-needed",
  "jsxBracketSameLine": false,
  "htmlWhitespaceSensitivity": "css",
  "overrides": [
    {
      "files": ".ts,.tsx",
      "options": {
        "importOrder": ["^react", "^[./]"],
        "importOrderSeparation": true,
        "importOrderSortSpecifiers": true
      }
    }
  ]
}
